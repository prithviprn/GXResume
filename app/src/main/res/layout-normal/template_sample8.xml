                        android:layout_height="@dimen/image_hight_normal"
                        android:layout_marginLeft="@dimen/image_margin_left_normal"
                        android:layout_marginRight="@dimen/image_margin_right_normal"
                        android:background="@android:color/white"
                        android:src="@drawable/ic_achievement_hollow" />

                    <com.grexoft.resume.widgets.TextViewWithFont
                        android:id="@+id/txt_achievements"
                        font:name="oswald"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="@string/achievements"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/text_size_head_normal"
                        android:textStyle="bold"
                        font:spacing="@dimen/spacing_head"
                        font:type="bold" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layout_write_achievements"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:clipChildren="false"
                    android:orientation="vertical"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_size_normal" >
