                <!-- android:clipChildren="false" -->
                <!-- android:textSize="@dimen/text_size_normal" -->
                <!-- android:textColor="@android:color/black" > -->
                <!-- </LinearLayout> -->
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_reference_parent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/left_border_normal"
                android:clipChildren="false"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/layout_reference_child"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/image_reference"
                        android:layout_width="@dimen/image_width_normal"
                        android:layout_height="@dimen/image_hight_normal"
                        android:layout_marginLeft="@dimen/image_margin_left_normal"
                        android:layout_marginRight="@dimen/image_margin_right_normal"
                        android:background="@android:color/white"
                        android:src="@drawable/ic_reference_hollow" />

                    <com.grexoft.resume.widgets.TextViewWithFont
                        android:id="@+id/txt_heading_reference"
                        font:name="oswald"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="@string/references"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/text_size_head_normal"
                        android:textStyle="bold"
                        font:spacing="@dimen/spacing_head"
                        font:type="bold" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layout_write_reference"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clipChildren="false"
                    android:orientation="vertical"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_size_normal" >
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_declaration_parent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@android:color/black" />

                <LinearLayout
                    android:id="@+id/layout_declaration_child1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/image_declaration"
                        android:layout_width="@dimen/image_width_normal"
                        android:layout_height="@dimen/image_hight_normal"
                        android:layout_marginRight="10dp"
