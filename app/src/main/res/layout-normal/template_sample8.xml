                        font:spacing="@dimen/spacing_head"
                        font:type="bold" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layout_declaration_child2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/txt_write_declaration"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/declaration_details"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/text_size_normal" />
                </LinearLayout>
            </LinearLayout>

            <com.grexoft.resume.widgets.TextViewWithFont
                android:id="@+id/txt_date"
                font:name="oswald"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:text="@string/date"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_size_normal"
                font:spacing="@dimen/spacing_head" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:orientation="horizontal"
                android:weightSum="1.0" >

                <com.grexoft.resume.widgets.TextViewWithFont
                    android:id="@+id/txt_place"
                    font:name="oswald"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/place"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_size_normal"
                    font:spacing="@dimen/spacing_head" />

                <TextView
                    android:id="@+id/txt_signature"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:gravity="right"
                    android:text="@string/signature"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/text_size_normal" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

