                    <ImageView
                        android:id="@+id/image_personal_details"
                        android:layout_width="@dimen/image_width_normal"
                        android:layout_height="@dimen/image_hight_normal"
                        android:layout_marginLeft="@dimen/image_margin_left_normal"
                        android:layout_marginRight="@dimen/image_margin_right_normal"
                        android:background="@android:color/white"
                        android:src="@drawable/ic_personal_hollow" />

                    <com.grexoft.resume.widgets.TextViewWithFont
                        android:id="@+id/txt_heading_personal_details"
                        font:name="oswald"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="@string/personal_details"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/text_size_head_normal"
                        android:textStyle="bold"
                        font:spacing="@dimen/spacing_head"
                        font:type="bold" />
                </LinearLayout>

                <!-- <LinearLayout -->
                <!-- android:id="@+id/layout_wtite_personal_details" -->
